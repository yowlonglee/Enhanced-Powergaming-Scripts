	//==========================================================================
	LPF REPLACE_MULTILINE
		INT_VAR
		num = 1
		strict = 1
		verbose = 0
		STR_VAR
		pattern = ~~~~~IF
  OR(2)
    Global("MO_BUFF","LOCALS",1)
    Global("MO_LONG_BUFF","LOCALS",1)
THEN
  RESPONSE #100
    SetGlobal("MO_BUFF","LOCALS",0)
    SetGlobal("MO_LONG_BUFF","LOCALS",0)
    DisplayStringHead(Myself,@5004)
END~~~~~
		string = ~~~~~IF
  OR(2)
    Global("MO_BUFF","LOCALS",1)
    Global("MO_LONG_BUFF","LOCALS",1)
THEN
  RESPONSE #100
