  !CheckSpellState(Myself,WILD_MAGIC_AREA)
  !CheckSpellState(Myself,DEAD_MAGIC_AREA)
THEN
  RESPONSE #100
    ReallyForceSpellRES(WIZARD_STONE_SKIN,Myself)
    SetGlobalTimer("MO_SpellCast","LOCALS",3)
    //RemoveSpellRES(SPWI408)
END
~~~~~ //"
  END

